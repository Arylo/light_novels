{
    "title" : "Arylo 的藏书阁",
    "description": "收集已翻译的Web轻小说，方便未来时复读",
    "author": "AryloYeung",
    "language" : "zh-hans",
    "plugins": [
        "-highlight",
        "-sharing",
        "-lunr",
        "-search",
        "-fontsettings",
        "scripts",
        "search-pro",
        "back-to-top-button",
        "pageview-count",
        "flexible-alerts",
        "lightbox"
    ],
    "pluginsConfig": {
        "flexible-alerts": {
            "system": {
              "label": "System",
              "icon": "fa fa-comments",
              "className": "info"
            },
            "status": {
              "label": "Status",
              "icon": "fa fa-info",
              "className": "info"
            }
        },
        "scripts": {
            "files": [
                "./script.js"
            ]
        }
    },
    "styles": {
        "website": "style.css"
    },
    "links": {
        "Github": "https://github.com/arylo",
        "Repository": "https://github.com/Arylo/light_novels"
    },
    "root": "."
}